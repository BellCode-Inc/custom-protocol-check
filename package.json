{
  "name": "custom-protocol-check",
  "version": "1.0.3",
  "description": "Detect whether a custom protocol is available in browser (Chrome, Firefox, Safari, iOS, IE8-IE11 and Edge)",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "start": "npm run build && webpack-dev-server --config ./webpack.config.js",
    "test": "start-server-and-test start http://localhost:9999 test:protractor",
    "test:protractor": "protractor protractor.conf.js",
    "postinstall": "webdriver-manager update && node ./node_modules/protractor/bin/webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vireshshah/custom-protocol-check.git"
  },
  "keywords": [
    "custom protocol check",
    "custom protocol detection",
    "javascript protocol check",
    "javascript custom protocol",
    "browser protocol support"
  ],
  "tags": [
    "custom protocol check",
    "custom protocol detection",
    "javascript protocol check",
    "javascript custom protocol",
    "browser protocol support"
  ],
  "maintainers": [
    {
      "name": "Ismail Habib Muhammad",
      "url": "https://github.com/ismailhabi"
    },
    {
      "name": "Viresh Shah",
      "email": "vireshshah@gmail.com",
      "url": "https://github.com/vireshshah"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vireshshah/custom-protocol-check/issues"
  },
  "homepage": "https://github.com/vireshshah/custom-protocol-check#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.5",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.37.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "yargs": "^13.3.0",
    "webpack-dev-server": "^3.7.2",
    "start-server-and-test": "^1.9.1",
    "protractor": "^5.4.2",
    "chromedriver": "^75.1.0",
    "webdriver-manager": "^12.1.5",
    "jasmine-core": "^3.4.0",
    "jasmine_dom_matchers": "^1.4.0",
    "jasmine-spec-reporter": "^4.2.1"
  }
}
